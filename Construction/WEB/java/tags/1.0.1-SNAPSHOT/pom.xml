			<artifactId>dao</artifactId>
			<version>0.13.1-SNAPSHOT</version>
		</dependency>

		<dependency>
			<groupId>framework</groupId>
			<artifactId>template</artifactId>
			<version>0.7.1-SNAPSHOT</version>
		</dependency>

		<!-- Framework -->

	</dependencies>

</project>
